                  {{'help-page.start.step-two.fill-in.title' | translate}}
                  ul.fa-ul
                    li 
                      i.fa.fa-angle-right.fa-li
                      i.fa.fa-angle-right.fa-li
                      {{'help-page.start.step-two.fill-in.location-info' | translate}}
