                      {{'help-page.start.step-two.fill-in.auth-methods' | translate}}
                    li 
                      i.fa.fa-angle-right.fa-li
                      i.fa.fa-angle-right.fa-li
                      {{'help-page.start.step-two.fill-in.login-design' | translate}}
                li 
                  i.fa.fa-angle-right.fa-li
                  {{'help-page.start.step-two.save-settings' | translate}}
            li
              h5 {{'help-page.start.step-three.title' | translate}}
