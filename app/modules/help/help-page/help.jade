          h3 {{ 'help-page.start.welcome' | translate }}
          h4 {{'help-page.start.h2' | translate}}
          ol
            li
              h5 {{'help-page.start.step-one.title' | translate}}
              ul.fa-ul
                h6 {{'help-page.start.step-one.reset-router.title' | translate}}
