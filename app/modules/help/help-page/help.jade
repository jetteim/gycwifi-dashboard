      .block.block-bordered.remove-margin.remove-padding.push-10-t#how-to-setup
        .block-header.bg-gray-lighter
          h2.block-title {{ 'help-page.control_center.additional_info' | translate }}
        .block-content.block-content-full
          ul.nav.nav-tabs.nav-tabs-alt.nav-justified(data-toggle='tabs')
            li
              a(href='#btabs-static-justified-self-setup')
                | {{ 'help-page.control_center.self-setup.title' | translate }}
            li.active
              a(href='#btabs-static-justified-call-for-service')
                | {{ 'help-page.control_center.call-for-service.title' | translate }}
            li
              a(href='#btabs-static-justified-tech-info')
                | {{ 'help-page.control_center.tech-info.title' | translate }}
          .block-content.tab-content
            #btabs-static-justified-self-setup.tab-pane
              h3.push-15.push-15-t.text-center
                | {{ 'help-page.control_center.self-setup.mikrotik-self-start.title' | translate }}
              .prepare-router.push-15.push-15-t
                ul.fa-ul
                  h4
                    | {{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.title' | translate }}
                  li.reset-router
                    i.fa.fa-check.fa-li
                    h5 {{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.reset_router.reset' | translate }}
                    ul.fa-ul
                      li
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.reset_router.step1' | translate }}
                      li
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.reset_router.step2' | translate }}
                      li
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.reset_router.step3' | translate }}
                      li
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.reset_router.step4' | translate }}
                  li.update-router
                    i.fa.fa-check.fa-li
                    h5 {{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.update_router.update' | translate }}
                    ul.fa-ul
                      li 
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.update_router.step1' | translate }}
                      li 
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.update_router.step2-begin' | translate }}
                        a(href="{{'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.webfig' | translate}}")
                          |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.webfig' | translate }}
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.update_router.step2-end' | translate }}
                      li
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.prepare_router.update_router.step3' | translate }}
              .setup_router.push-15.push-15-t
                ul.fa-ul
                  h4
                    |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.title' | translate }}
                  li.add-router
                    i.fa.fa-check.fa-li
                    |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.add_router.add' | translate }}
                    ul.fa-ul
                      li 
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.add_router.step1' | translate }}
                      li 
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.add_router.step2' | translate }}
                      li 
                        i.fa.fa-angle-right.fa-li
                        |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.add_router.step3' | translate }}
                  li.download-package
                    i.fa.fa-check.fa-li
                    |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.download_package' | translate }}
                  li.run-setup
                    i.fa.fa-check.fa-li
                    |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.run_setup' | translate }}
                  li.final-steps 
                    i.fa.fa-check.fa-li
                    |{{ 'help-page.control_center.self-setup.mikrotik-self-start.setup_router.final_step' | translate }}
            #btabs-static-justified-call-for-service.tab-pane.active
              .youdo.push-15.push-15-t
                h5.push-15
                  |{{ 'help-page.control_center.call-for-service.youdo-begin' | translate }}
                  a(href="{{ 'help-pagecontrol_center.call-for-service.youdo-link' | translate}}", target = "_blank")
                    |{{ 'help-page.control_center.call-for-service.youdo-link' | translate }}
                  |{{ 'help-page.control_center.call-for-service.youdo-end' | translate }}
                ul.fa-ul
                  li 
                    i.fa.fa-angle-right.fa-li
                    |{{ 'help-page.control_center.call-for-service.step1' | translate }}
                  li 
                    i.fa.fa-angle-right.fa-li
                    |{{ 'help-page.control_center.call-for-service.step2' | translate }}
                  li 
                    i.fa.fa-angle-right.fa-li
                    |{{ 'help-page.control_center.call-for-service.step3' | translate }}
                  li 
                    i.fa.fa-angle-right.fa-li
                    |{{ 'help-page.control_center.call-for-service.step4' | translate }}
                    a(href="mailto:{{ 'help-page.control_center.info-email | translate' }}")
                      |{{ 'help-page.control_center.info-email' | translate }}
            #btabs-static-justified-tech-info.tab-pane
              h5.push-15.push-15-t.text-center
                |{{ 'help-page.control_center.tech-info.title' | translate }}
              .router-package.push-15.push-15-t
                p
                  |{{ 'help-page.control_center.tech-info.router-package.package-text1' | translate }}
                  a(href="{{'help-page.control_center.tech-info.router-package.package-url' | translate}}", target = "_blank")
                    |{{ 'help-page.control_center.tech-info.router-package.package-url' | translate }}
                  |{{ 'help-page.control_center.tech-info.router-package.package-text2' | translate }}
              .vpn-link.push-15.push-15-t
                p
                  |{{ 'help-page.control_center.tech-info.vpn-link.text1' | translate }}
                  a(href="{{'help-page.control_center.tech-info.vpn-server' | translate}}", target = '_blank')
                    |{{ 'help-page.control_center.tech-info.vpn-server' | translate }}
                  |{{ 'help-page.control_center.tech-info.vpn-link.text2' | translate }}
              .hotspot.push-15.push-15-t
                p
                  |{{ 'help-page.control_center.tech-info.hotspot.text1' | translate }}
                p
                  |{{ 'help-page.control_center.tech-info.hotspot.text2-begin' | translate }}
                  a(href="{{'control_center.tech-info.login-server' | translate}}", target = '_blank')
                    |{{ 'help-page.control_center.tech-info.login-server' | translate }}
                  |{{ 'control_center.tech-info.hotspot.text2-end' | translate }}
